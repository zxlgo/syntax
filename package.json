{
  "name": "zx",
  "displayName": "Zx Language",
  "description": "Zx Language Syntax Highlighting",
  "version": "0.0.2",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zx",
        "aliases": [
          "zxl",
          "zx"
        ],
        "extensions": [
          ".zx"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/zx.svg",
          "dark": "./icons/zx.svg"
        }
      }
    ],
    "snippets": [
      {
        "language": "zx",
        "path": "./snippets/hello.json"
      },
      {
        "language": "zx",
        "path": "./snippets/error.json"
      },
      {
        "language": "zx",
        "path": "./snippets/define.json"
      },
      {
        "language": "zx",
        "path": "./snippets/namespace.json"
      },
      {
        "language": "zx",
        "path": "./snippets/html.json"
      },
      {
        "language": "zx",
        "path": "./snippets/fn.json"
      },
      {
        "language": "zx",
        "path": "./snippets/statements.json"
      }
    ],
    "grammars": [
      {
        "language": "zx",
        "scopeName": "source.zx",
        "path": "./syntaxes/zx.tmLanguage.json"
      }
    ]
  }
}
